<!DOCTYPE html>
<html>
<body>
	<a  href="/"><button class="home">Back TO Home</button></a>
	<h2>finded data</h2>
	<div>
		<table border="1" bgcolor="lightblue">
			<tr>
				<th>Name</th>
				<th>Id</th>
				<th>Age</th>
				<th>Salary</th>
				<th>phone</th>
			</tr>
			<% for(var i=0; i < userdata.length; i++) { %>
			<tr>
				<td><%= userdata[i].name %></td>
				<td><%= userdata[i].id %></td>
				<td><%= userdata[i].age %></td>
				<td><%= userdata[i].salary %></td>
				<td><%= userdata[i].phone %></td>
			</tr>
			<% } %>
			
		</table>
	</div>
	<style type="text/css">
		.home{
			position: absolute;
    		margin-left: 70rem;
            background-color: #FFB6C1;
    		color: red;
            font-size: 20px;
            width: 15%;
            height: 40px;
    	}
	</style>
</body>
</html>